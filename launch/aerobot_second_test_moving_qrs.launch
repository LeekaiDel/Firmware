<?xml version="1.0"?>
<launch>
    <!-- MAVROS posix SITL environment launch script -->
    <!-- launches MAVROS, PX4 SITL, Gazebo environment, and spawns vehicle -->
    <!-- vehicle pose -->
    <arg name="x" default="-6.2"/>
    <arg name="y" default="6.5"/>
    <arg name="z" default="0.4"/>
    <arg name="R" default="0"/>
    <arg name="P" default="0"/>
    <arg name="Y" default="0"/>
    <!-- vehicle model and world -->
    <arg name="est" default="ekf2"/>
    <arg name="vehicle" default="iris_rplidar"/>
    <arg name="world" default="$(find mavlink_sitl_gazebo)/worlds/second_scene_world_v1.world"/>
    <arg name="sdf" default="$(find mavlink_sitl_gazebo)/models/$(arg vehicle)/$(arg vehicle).sdf"/>

    <!-- gazebo configs -->
    <arg name="gui" default="true"/>
    <arg name="debug" default="false"/>
    <arg name="verbose" default="false"/>
    <arg name="paused" default="false"/>
    <arg name="respawn_gazebo" default="false"/>
    <!-- MAVROS configs -->
    <arg name="fcu_url" default="udp://:14540@localhost:14557"/>
    <arg name="respawn_mavros" default="false"/>
    <!-- PX4 configs -->
    <arg name="interactive" default="true"/>
    <!-- PX4 SITL and Gazebo -->
    <include file="$(find px4)/launch/posix_sitl.launch">
        <arg name="x" value="$(arg x)"/>
        <arg name="y" value="$(arg y)"/>
        <arg name="z" value="$(arg z)"/>
        <arg name="R" value="$(arg R)"/>
        <arg name="P" value="$(arg P)"/>
        <arg name="Y" value="$(arg Y)"/>
        <arg name="world" value="$(arg world)"/>
        <arg name="vehicle" value="$(arg vehicle)"/>
        <arg name="sdf" value="$(arg sdf)"/>
        <arg name="gui" value="$(arg gui)"/>
        <arg name="interactive" value="$(arg interactive)"/>
        <arg name="debug" value="$(arg debug)"/>
        <arg name="verbose" value="$(arg verbose)"/>
        <arg name="paused" value="$(arg paused)"/>
        <arg name="respawn_gazebo" value="$(arg respawn_gazebo)"/>
    </include>

    <node name="object_mover_2" pkg="object_mover_gazebo" type="object_mover" args="2qr 0 17.5 0 2.1 0.4 30" output="log"/>
    <node name="object_mover_3" pkg="object_mover_gazebo" type="object_mover" args="3qr 5 17.5 0 2.1 0.4 30" output="log"/>
    <node name="object_mover_4" pkg="object_mover_gazebo" type="object_mover" args="4qr -5 12.5 0 2.1 0.4 30" output="log"/>
    <node name="object_mover_5" pkg="object_mover_gazebo" type="object_mover" args="5qr 0 12.5 0 2.1 0.4 30" output="log"/>
    <node name="object_mover_6" pkg="object_mover_gazebo" type="object_mover" args="6qr 5 12.5 0 2.1 0.4 30" output="log"/>
    <node name="object_mover_7" pkg="object_mover_gazebo" type="object_mover" args="7qr -4.25 8 0 1.25 0.4 30" output="log"/>
    <node name="object_mover_8" pkg="object_mover_gazebo" type="object_mover" args="8qr 0 7.5 0 2.1 0.4 30" output="log"/>
    <node name="object_mover_9" pkg="object_mover_gazebo" type="object_mover" args="9qr 5 7.5 0 2.1 0.4 30" output="log"/>

    <!-- MAVROS -->
    <!-- <include file="$(find mavros)/launch/px4.launch">
        <arg name="gcs_url" value=""/>
        <arg name="fcu_url" value="$(arg fcu_url)"/>
        <arg name="respawn_mavros" value="$(arg respawn_mavros)"/>
    </include> -->
</launch>
